6. Linked List - Appending a Book to a Library Catalog
import java.util.*;
class Librarynode{
    int id;
    Librarynode next;
    
    public Librarynode(int id){
        this.id=id;
        this.next=null;
    }
}

public class Solution {
    Librarynode head;
    
    public void insertatend(int id){
        Librarynode newNode= new Librarynode(id);
        if(head==null){
            head=newNode;
        }
        else{
            Librarynode temp=head;
            while(temp.next!=null){
                temp=temp.next;
            }
            temp.next=newNode;
        }
    }
    
    public void display(){
        if(head==null){
            System.out.println("No IDs are there");
            return;
            
        }
        Librarynode temp=head;
        while(temp!=null){
            System.out.print(temp.id+" ");
            temp=temp.next;
        }
        System.out.println();
    }
    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc=new Scanner(System.in);
        Solution q=new Solution();
        
        int n=sc.nextInt();
        for(int i=0;i<n;i++){
            int id=sc.nextInt();
            q.insertatend(id);
            
        }
        q.display();
    }
    
}
