56. Binary Search Tree - Organizing a Library Collection

import java.io.*;
import java.util.*;
class Node{
    int value;
    Node left, right;
    
    Node(int value)
    {
        this.value=value;
        this.left=null;
        this.right=null;
        
    }
}
public class Solution {
    
    public static Node insert(Node root,int value)
    {
        if(root==null)
        {
            return new Node(value);
        }
        if(value<root.value)
        {
            root.left=insert(root.left,value);
        }
        else
        {
            root.right=insert(root.right,value);
        }
        return root;
    }

    public static void inorder(Node root)
    {
        if (root==null)
        {
            return;
        }
        
        inorder(root.left);
        System.out.print(root.value+" ");
        inorder(root.right);
    }
    public static void main(String[] args) {
        
        
        Scanner sc=new Scanner(System.in);
        int n = sc.nextInt();
        Node root=null;
        for(int i=0;i<n;i++)
        {
            int val=sc.nextInt();
            root=insert(root,val);
        }
        inorder(root);
    } 
}
