7. Mathematical Algorithms - Prime Factorization of a Number

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        
       for(int i=2;i<=n;i++)
        {
           while(n%i==0)
           {
            System.out.print(i+" ");
            n=n/i;
           }
        }
        
    }
}
