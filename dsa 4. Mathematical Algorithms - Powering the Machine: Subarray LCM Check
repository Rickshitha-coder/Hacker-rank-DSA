4. Mathematical Algorithms - Powering the Machine: Subarray LCM Check

import java.io.*;
import java.util.*;

public class Solution {
    
    static long gcd(long a, long b)//1 3
    {
        if(b==0) //
            return a;
        return gcd(b,a%b);
    }

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        
        int n=sc.nextInt();
        
        long k=sc.nextLong(); //long-- more digits like 10
        
        long[] num= new long[n];
        
        for(int i=0;i<n;i++)
        {
          num[i]=sc.nextLong();
        }
        
        int count=0;
        
        for(int i=0;i<n;i++)  //i=0
        {
            long currentLCM =1; //initialise 1 for very start  //1 3
            for(int j=i;j<n;j++)  //j=0  //j=1  //j=2
            {
                long gcd = gcd(currentLCM, num[j]);  
                currentLCM = (currentLCM * num[j]) / gcd; 
                
                if(currentLCM > k) 
                    break;
                if(currentLCM ==k) 
                    count++; 
            }
        }
     System.out.print(count);
    }
}
