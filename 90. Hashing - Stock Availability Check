90. Hashing - Stock Availability Check


import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
        
        int m=sc.nextInt();
        int n=sc.nextInt();
        HashSet<Integer> set=new HashSet<>();
        for(int i=0;i<m;i++)
        {
            int id=sc.nextInt();
            set.add(id);
        }
        
        boolean issubset=true;
        
        for(int i=0;i<n;i++)
        {
            int product=sc.nextInt();
            if(!set.contains(product))
            {
                issubset=false;
                break;
                
            }
        }
        
        if(issubset)
        {
            System.out.print("Yes");
        }
        else
        {
            System.out.print("No");
        }
    }
}
