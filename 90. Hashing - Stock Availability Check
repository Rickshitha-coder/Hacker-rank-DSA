90. Hashing - Stock Availability Check


import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int m = sc.nextInt(); // inventory size
        int n = sc.nextInt(); // order size

        int[] inventory = new int[m];
        int[] order = new int[n];

        // Read inventory array
        for (int i = 0; i < m; i++) {
            inventory[i] = sc.nextInt();
        }

        // Read order array
        for (int i = 0; i < n; i++) {
            order[i] = sc.nextInt();
        }

        // Use HashSet for inventory
        HashSet<Integer> set = new HashSet<>();
        for (int i = 0; i < m; i++) {
            set.add(inventory[i]);
        }

        boolean isSubset = true;

        // Check if order is subset of inventory
        for (int i = 0; i < n; i++) {
            if (!set.contains(order[i])) {
                isSubset = false;
                break;
            }
        }

        if (isSubset) {
            System.out.println("Yes");
        } else {
            System.out.println("No");
        }

      
    }
}
