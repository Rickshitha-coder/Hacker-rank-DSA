24. Searching - Log Analysis: Finding First and Last Occurrence of an Event


import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt(); // 6
        int[] arr = new int[n];
        
        for(int i = 0; i < n; i++)
        {
            arr[i] = sc.nextInt(); // 5 7 7 8 8 10
        }  
        
        int x = sc.nextInt(); // 8
        
        int first = -1; // 3
        int last = -1;  // 4
        
        for(int i = 0; i < n; i++)//0  1 2 3
        {
            if(arr[i] == x) //arr[0]==8  5!=8 //7!=8 //7!=8 //8==8
            {
                first = i; //first=3
                break;
            }
        }

        for(int i = n - 1; i >= 0; i--)//i=6-1=5   //i=4
        {
            if(arr[i] == x)//arr[5]==8  10!=8  //8==8
            {
                last = i;//last=4
                break;
            }
        }
        
        System.out.println("[" + first + ", " + last + "]"); // [3, 4]
    }
}
