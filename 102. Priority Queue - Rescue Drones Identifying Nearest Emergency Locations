102. Priority Queue - Rescue Drones Identifying Nearest Emergency Locations


import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        
        int[][] points=new int[n][3];
        
        for(int i=0;i<n;i++)
        {
            int x=sc.nextInt();
            int y=sc.nextInt();
            
            int dist=x*x + y*y;  //1*1 +3*3 =10
            
            points[i][0]=x; //1
            points[i][1]=y; //3
            points[i][2]=dist;  //10
            
            
        }//read data from the user and calculate the difference
        
        int k=sc.nextInt();
        
        Arrays.sort(points, (a,b) -> a[2]-b[2]);
        
        for(int i=0;i<k;i++)
        {
            System.out.println(points[i][0]+" "+points[i][1]);
        }
    }
}
