7.Linked List - Removing the Last Book from a Library Catalog
import java.util.*;

class Node {
    int id;
    Node next, prev;

    Node(int id) {
        this.id = id;
        this.prev = null;
        this.next = null;
    }
}

public class Solution {
    Node head;

    // Insert node at the end
    public void insertAtEnd(int id) {
        Node newNode = new Node(id);
        if (head == null) {
            head = newNode;
            return;
        }
        Node temp = head;
        while (temp.next != null) {
            temp = temp.next;
        }
        temp.next = newNode;
        newNode.prev = temp;
    }

    // Delete last node
    public void deleteAtEnd() {
        if (head == null) {
            return; // list empty
        }
        if (head.next == null) {
            head = null; // only one node
            return;
        }

        Node temp = head;
        while (temp.next != null) {
            temp = temp.next;
        }
        temp.prev.next = null; // unlink last node
    }

    // Display list
    public void display() {
        if (head == null) {
            System.out.println("List is empty");
            return;
        }
        Node temp = head;
        while (temp != null) {
            System.out.print(temp.id);
            if (temp.next != null) {
                System.out.print(" ");
            }
            temp = temp.next;
        }
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Solution dll = new Solution();

        int N = sc.nextInt();  // number of books
        for (int i = 0; i < N; i++) {
            dll.insertAtEnd(sc.nextInt());
        }

        // Delete the last book
        dll.deleteAtEnd();

        // Print updated catalog
        dll.display();

      
    }
}
