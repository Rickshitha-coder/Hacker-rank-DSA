10. Mathematical Algorithms - Jumbled Number Validator


import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String n = sc.next();

        boolean isJumbled = true;   // flag

        for(int i = 0; i < n.length() - 1; i++) {
            int d1 = n.charAt(i) - '0';
            int d2 = n.charAt(i + 1) - '0';

            int diff = Math.abs(d1 - d2);

            if(diff > 1) {
                isJumbled = false;
                break;   
            }
        }

        if(isJumbled) {
            System.out.print("Jumbled");
        } else {
            System.out.print("Not Jumbled");
        }
    }
}
