Date : 31/12/2025 
95. Hashing - Group Inventory Items by First Appearance


import java.util.*;

public class Solution {
    public static void main(String[] args) {

        // Scanner object to read input
        Scanner sc = new Scanner(System.in);

        // Read number of inventory items
        int n = sc.nextInt();  
        // Example: n = 6

        // LinkedHashMap stores:
        // key   - item code
        // value - frequency (count)
        // It also preserves insertion (first appearance) order
        LinkedHashMap<Integer, Integer> map = new LinkedHashMap<>();

        // Read each inventory item
        for (int i = 0; i < n; i++) {

            int item = sc.nextInt();
            // Items read one by one:
            // i=0  item = 101
            // i=1  item = 102
            // i=2  item = 103
            // i=3  item = 101
            // i=4  item = 102
            // i=5  item = 101

            // If item is already present, increase count
            // Otherwise, insert it with count = 1
            map.put(item, map.getOrDefault(item, 0) + 1);

            // Map state after each step:
            // After 101  {101=1}
            // After 102  {101=1, 102=1}
            // After 103  {101=1, 102=1, 103=1}
            // After 101  {101=2, 102=1, 103=1}
            // After 102  {101=2, 102=2, 103=1}
            // After 101  {101=3, 102=2, 103=1}
        }

        // Traverse map in order of first appearance
        for (Map.Entry<Integer, Integer> entry : map.entrySet()) {

            int item = entry.getKey();      // item code 101  102 103
            int count = entry.getValue();   // number of occurrences 3 2 1

            // Print the item 'count' times
            for (int i = 0; i < count; i++) {  //i=0 ,i<3 i=1 i=2 //i=0 i<2 i=1 //i=0 i<1
                System.out.print(item + " "); //101 101 101 102 102 103
            }
        }
    }
}
