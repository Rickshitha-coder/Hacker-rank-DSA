1.	Linked List - Product ID Insertion in Inventory

import java.util.*;
class Productnode{
    int prodid;
    Productnode next;
    
    public Productnode(int prodid){
        this.prodid=prodid;
        this.next=null;
    }
}

public class Solution {
    Productnode head;
    
    public void insertatend(int prodid){
        Productnode newNode= new Productnode(prodid);
        if(head==null){
            head=newNode;
        }
        else{
            Productnode temp=head;
            while(temp.next!=null){
                temp=temp.next;
            }
            temp.next=newNode;
        }
    }
    
    public void display(){
        if(head==null){
            System.out.println("No IDs are there");
            return;
            
        }
        Productnode temp=head;
        while(temp!=null){
            System.out.print(temp.prodid+" ");
            temp=temp.next;
        }
        System.out.println();
    }
    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc=new Scanner(System.in);
        Solution q=new Solution();
        
        int n=sc.nextInt();
        for(int i=0;i<n;i++){
            int id=sc.nextInt();
            q.insertatend(id);
            
        }
        q.display();
    }
    
}
