91. Hashing - Product Variety in Moving Window of Time


import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        int n= sc.nextInt();  //total products
        int k=sc.nextInt();   //size of subarray
        
        int[] arr=new int[n];
        
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        
        
        for(int i=0;i<=n-k;i++)  //0 < 8-5
        {
            HashSet <Integer >set=new HashSet<>();  //to store distinct values

            for(int j=i;j<(i+k);j++)
            {
                set.add(arr[j]);
            }
            System.out.println(set.size());
        }
        
    }
}
